{
  "name": "nextjs-anchor",
  "description": "Next.js, Tailwind, @solana/react-hooks, Anchor vault program",
  "keywords": [
    "anchor",
    "kit",
    "nextjs",
    "react",
    "solana-kit",
    "tailwind",
    "typescript"
  ],
  "create-solana-dapp": {
    "rename": {
      "scaffold": {
        "to": "{{name}}",
        "paths": [
          "app"
        ]
      }
    }
  },
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "anchor-build": "cd anchor && anchor build",
    "anchor-test": "cd anchor && anchor test --skip-deploy",
    "build": "next build",
    "ci": "npm run build && npm run lint && npm run format:check",
    "codama:js": "codama run js",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint",
    "setup": "npm run anchor-build && npm run codama:js",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@drift-labs/sdk": "^2.100.0",
    "@jup-ag/api": "^6.0.36",
    "@lightprotocol/compressed-token": "^0.22.0",
    "@lightprotocol/stateless.js": "^0.22.0",
    "@magicblock-labs/ephemeral-rollups-sdk": "^0.6.5",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^1.6.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@solana/client": "^1.2.0",
    "@solana/kit": "^5.1.0",
    "@solana/react-hooks": "^1.1.5",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@types/three": "^0.182.0",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "chart.js": "^4.5.1",
    "eciesjs": "^0.4.14",
    "framer-motion": "^12.29.2",
    "lightweight-charts": "^5.1.0",
    "lucide-react": "^0.563.0",
    "next": "15.5.9",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.3",
    "three": "^0.182.0",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.3",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.3.8",
    "@codama/renderers-js": "^1.5.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.2",
    "@types/bn.js": "^5.1.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "codama": "^1.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "jsdom": "^27.4.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
